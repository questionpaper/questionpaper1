<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app id="WebApp_ID">
	<display-name>LoginApp</display-name>
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<display-name>LoginServlet</display-name>
		<description></description>
		<servlet-class>com.cts.loginapp.controller.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>registerDAOImpl</servlet-name>
		<display-name>registerDAOImpl</display-name>
		<description></description>
		<servlet-class>com.cts.loginapp.dao.registerDAOImpl</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>RegisterServlet</servlet-name>
		<display-name>RegisterServlet</display-name>
		<description></description>
		<servlet-class>com.cts.loginapp.controller.RegisterServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>RegisterServiceImpl</servlet-name>
		<display-name>RegisterServiceImpl</display-name>
		<description></description>
		<servlet-class>com.cts.loginapp.service.RegisterServiceImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/LoginServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>registerDAOImpl</servlet-name>
		<url-pattern>/registerDAOImpl</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>RegisterServlet</servlet-name>
		<url-pattern>/RegisterServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>RegisterServiceImpl</servlet-name>
		<url-pattern>/RegisterServiceImpl</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>loginPage.jsp</welcome-file>
		
	</welcome-file-list>
</web-app>
